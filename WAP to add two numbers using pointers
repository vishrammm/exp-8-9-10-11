#include <stdio.h>

int main() {
    int a, b, sum;
    int *p1, *p2;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    p1 = &a;   // pointer points to a
    p2 = &b;   // pointer points to b

    sum = *p1 + *p2;   // add values using pointers

    printf("Sum = %d", sum);

    return 0;
}
